package KUSH;
import java.util.Scanner;
public class concentratestring {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
                String str1 = sc.nextLine();
                String str2 = sc.nextLine();
                String str3 = str1.concat(str2);
                String str4 = sc.nextLine();
                String str5 = sc.nextLine();
                String str6 = str4.concat(str5);
                String str7 = sc.nextLine();
                String str8 = sc.nextLine();
                String str9 = str7.concat(str8);
                String str10 = sc.nextLine();
                String str11 = sc.nextLine();
                String str12 = str10.concat(str11);
                System.out.println(str3);
                System.out.println(str6);
                System.out.println(str9);
                System.out.println(str12);
    }
}


